{
    "manifest_version": 3,
    "name": "Commently",
    "version": "1.0.0",
    "description": "A simple React app as a Chrome extension",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs"
    ],
    "icons": {
        "48": "logo-color-48.png",
        "96": "logo-color-96.png"
    },
    "background": {
        "service_worker": "background.js",
        "scripts": ["background.js"]
    }
  }
